<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.gamehub_project.HigherOrLower">
   <children>
      <Button fx:id="StartButton" layoutX="14.0" layoutY="306.0" mnemonicParsing="false" onAction="#Start_Button_Clicked" prefHeight="80.0" prefWidth="123.0" text="Start" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="BackButton" layoutX="463.0" layoutY="306.0" mnemonicParsing="false" onAction="#switchToGameHub" prefHeight="80.0" prefWidth="123.0" text="Back" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <TextField fx:id="FirstCardFlip" alignment="CENTER" layoutX="63.0" layoutY="45.0" prefHeight="222.0" prefWidth="149.0" visible="false">
         <font>
            <Font size="48.0" />
         </font></TextField>
      <TextField fx:id="SecondCardFlip" alignment="CENTER" layoutX="389.0" layoutY="45.0" prefHeight="222.0" prefWidth="149.0" visible="false">
         <font>
            <Font size="48.0" />
         </font></TextField>
      <TextField fx:id="HigherOrLowerQuestionBox" alignment="CENTER" editable="false" layoutX="221.0" layoutY="58.0" prefHeight="39.0" prefWidth="157.0" text="Higher or Lower?" visible="false">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="HigherButton" layoutX="248.0" layoutY="127.0" mnemonicParsing="false" onAction="#Higher_Button_Clicked" prefHeight="58.0" prefWidth="103.0" text="Higher" textAlignment="CENTER" visible="false">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="LowerButton" layoutX="248.0" layoutY="209.0" mnemonicParsing="false" onAction="#Lower_Button_Clicked" prefHeight="58.0" prefWidth="103.0" text="Lower" visible="false">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="Play_Again_Button" layoutX="238.0" layoutY="306.0" mnemonicParsing="false" onAction="#Play_Again_Button_Clicked" prefHeight="80.0" prefWidth="123.0" text="Play Again?" textAlignment="CENTER" visible="false" wrapText="true">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
